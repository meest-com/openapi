{
    "name": "meest/openapi",
    "type": "library",
	"version": "dev-master",
    "description": "Meest open API",
	"keywords": [
        "meest", "api"
    ],
	"license": "MIT",
    "authors": [
        {
            "name": "volodya",
			"email": "webdeveloper@gmail.com"
        }
    ],
    "require": {
        "php": "~7.1",
        "guzzlehttp/guzzle": "^6.3"
    },
	"autoload": {
        "psr-4": {
            "Meest\\OpenApi\\": "src/"
        }
    },
	"autoload-dev": {
        "psr-4": {
            "Meest\\OpenApi\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
	"minimum-stability": "dev",
	"prefer-stable": true
}
